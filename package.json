{
  "name": "strata-vid",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "Browser-based video editor for stitching run videos with GPX data",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "npm run test:unit",
    "test:unit": "NODE_OPTIONS='--max-old-space-size=2048' jest --config jest.unit.config.js --runInBand",
    "test:integration": "NODE_OPTIONS='--max-old-space-size=2048' jest --config jest.integration.config.js --runInBand",
    "test:coverage": "NODE_OPTIONS='--max-old-space-size=2048' jest --config jest.unit.config.js --coverage --runInBand",
    "test:watch": "jest --config jest.unit.config.js --watch --runInBand",
    "type-check": "tsc --noEmit",
    "lint": "tsc --noEmit"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@types/geojson": "^7946.0.16",
    "@we-gold/gpxjs": "^1.1.0",
    "clsx": "^2.1.1",
    "immer": "^10.2.0",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.554.0",
    "mediabunny": "^1.25.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-leaflet": "^5.0.0",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@jest/globals": "^29.7.0",
    "@napi-rs/canvas": "^0.1.82",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.14",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.22",
    "babel-jest": "^30.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^27.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "ts-jest": "^29.2.5",
    "typescript": "~5.9.3",
    "vite": "^7.1.12",
    "xmldom-qsa": "^1.1.3"
  }
}
